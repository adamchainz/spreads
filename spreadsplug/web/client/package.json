{
  "name": "spreads-client",
  "version": "0.0.1",
  "description": "",
  "devDependencies": {
    "babel-core": "^4.7.16",
    "babel-loader": "^4.2.0",
    "css-loader": "^0.9.1",
    "eslint": "^0.17.1",
    "eslint-loader": "^0.8.0",
    "file-loader": "^0.8.1",
    "jsx-loader": "^0.12.2",
    "node-sass": "^3.0.0-pre",
    "raw-loader": "^0.5.1",
    "react-tools": "^0.13.1",
    "sass-loader": "^1.0.0",
    "url-loader": "^0.5.5",
    "webpack": "^1.7.3"
  },
  "dependencies": {
    "alt": "^0.14.5",
    "connect-fonts-opensans": "0.0.5",
    "font-awesome": "^4.3.0",
    "jquery": "^2.1.3",
    "lodash": "^3.6.0",
    "mousetrap": "^1.4.6",
    "react": "^0.13.1",
    "whatwg-fetch": "^0.7.0",
    "zurb-foundation": "https://github.com/zurb/foundation/archive/v5.5.1.tar.gz"
  },
  "eslintConfig": {
    "ecmaFeatures": {
      "arrowFunctions": true,
      "blockBindings": true,
      "classes": true,
      "destructuring": true,
      "modules": true
    },
    "env": {
      "browser": true
    },
    "globals": {
      "require": false,
      "module": false,
      "fetch": true
    },
    "rules": {
      "brace-style": "1tbs",
      "camelcase": 2,
      "comma-spacing": 2,
      "consistent-this": 2,
      "indent": [
        2,
        2
      ],
      "no-console": 0,
      "no-var": 2,
      "space-before-blocks": 2,
      "space-before-function-parentheses": [
        2,
        {
          "anonymous": "always",
          "named": "never"
        }
      ],
      "space-in-brackets": 2,
      "space-in-parens": 2,
      "space-infix-ops": 2,
      "valid-jsdoc": [
        2,
        {
          "prefer": {
            "return": "returns"
          },
          "requireParamDescription": false
        }
      ]
    }
  }
}
